{
  "name": "crm-backend",
  "version": "1.0.0",
  "private": true,
  "main": "server.js",
  "bin": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build:exe": "pkg ."
  },
  "dependencies": {
    "axios": "1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "http-proxy": "^1.18.1",
    "mssql": "^11.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "pkg": "^5.8.1"
  },
  "pkg": {
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist",
    "assets": [
        "swagger-routes/**/*.js",        
        "api/**/*",
        "node_modules/axios/package.json",
        "node_modules/axios/dist/**",
        "node_modules/swagger-ui-dist/**",
        "node_modules/swagger-ui-express/**"
      ]
  }
}
